    "json": "^9.0.3",
    "moment": "^2.11.2",
    "parcel": "^2.0.0-beta.1",
    "queue-async": "^1.2.1",
    "semver": "^5.1.0",
    "topojson": "^1.6.18"
  },
  "eslintConfig": {
    "extends": "planet"
  }
}
