{
   "manifest_version": 2,
   "name": "Planet View",
   "description": "Planet View displays a beautiful satellite image from Planet Labs every time you open a new tab.",
   "version": "0.3.0",
   "icons": {
      "128": "images/icon-128.png",
      "48": "images/icon-48.png",
      "16": "images/icon-16.png"
   },
   "permissions": [
      "https://www.planet.com/*"
   ],
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "incognito": "split"
}
