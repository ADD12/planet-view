{
  "manifest_version": 2,
  "name": "Planet View",
  "description": "Planet View displays a beautiful satellite image from Planet Labs every time you open a new tab.",
  "version": "0.8.0",
  "icons": {
    "16": "assets/images/icon-16.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  },
  "permissions": [
    "https://www.planet.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "incognito": "split"
}
